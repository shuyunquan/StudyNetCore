@model IEnumerable<Content>
@{
    ViewData["Title"] = "Index";
}

<h1>Content的Index页面</h1>

<table class="table table-hover">
    <thead>
        <tr>
            <td> <input type="checkbox" class="done-checkbox"></td>
            <td>序号</td>
            <td>标题</td>
            <td>内容</td>
            <td>添加时间</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <input type="checkbox" class="done-checkbox">
                </td>
                <td>@item.Id</td>
                <td>@item.title</td>
                <td>@item.content</td>
                <td>@item.add_time</td>
            </tr>
        }
    </tbody>
</table>
